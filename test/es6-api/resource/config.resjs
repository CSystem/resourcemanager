var fs = require("fs");

exports.groups = { "preload": ["bg_jpg"] };
exports.alias = { "bg_jpg": "assets/bg.jpg" };
exports.filter = function (p, plugins) {
	var ext = p.substr(p.lastIndexOf(".") + 1);
	var type;
	switch (ext) {
		case "json":
			if (p.indexOf("sheet") >= 0) {
				type = "sheet";
			}else if(p.indexOf("movieclip") >= 0){
				type = "movieclip";
			}else {
				type = "json";
			}
			break;
		case "png":
		case "jpg":
			type = "image";
			break;
		case "fnt":
			type = "font";
			break;
		case "mp3":
			type = "sound";
			break;
	}
	if (type) {
		var url = plugins.crc32(p) + "." + ext;
		return { url, type }
	}
	else {
		return null;
	}

}
